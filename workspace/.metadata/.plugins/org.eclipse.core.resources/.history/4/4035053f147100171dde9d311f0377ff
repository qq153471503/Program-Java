
public class ThreadDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		new TheMain("Íú²Æ").run();
	}

}



class Secondary extends Thread{
	
	private String name;
	
	Secondary(String name){
		this.name = name;
	}
	public void run() {
		this.work();
	}
	
	private void work(){
		for(int y=0; y<=10; y++)
		{
			for(int x=-99999999; x<=99999999; x++){
				System.out.println("y="+y+"...name:"+this.name);
			}
		}	
	}
}


class TheMain{
	private String name;
	
	TheMain(String name){
		this.name = name;
	}
	
	public void run(){
		for(int x=0; x<10; x++)
			System.out.println("x="+x+"...name:"+this.name);
	}
}


