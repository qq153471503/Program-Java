interface Graphics{
	abstract public double circularArea();
	abstract public double rectangularArea(); 
	abstract public double area();
}

class Circular implements Graphics{
	
	private double r;
	private double pai = 3.141592;
	
	public double area(){
		return this.r*this.r*this.pai;
	}
}


