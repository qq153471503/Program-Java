
class objectDemo{
	public static void main(String[] args){
		
		Persion per = new Persion(10);
		Persion per = new Persion("LiuAnkun");
	}
}

class Persion{
	private int num;
	public void setNum(int x){
		num = x;
	}
	
	Persion(){
		System.out.println("Persion run !");
	}
	Persion(int x){
		System.out.println("Persion run !"+"\t"+"x is :"+x);
	}
	Persion(String str){
		System.out.println("Persion run !"+"\t"+"String is :"+str);
		
	}
}

