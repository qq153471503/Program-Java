class Car{
	int num;
	String color;
	void carRun()
	{
		System.out.println("num is :"+num+"\t"+"color is £º "+color);
	}
}

class objectDemo{
	public static void main(String[] args){
		
		Car c1 = new Car();
		Car c2 = new Car();
		carBulid(c1, 10, "blue");
		carBulid(c2, 20, "red")
		System.out.println("object Demo !");
	}
	
	public static void carBulid(Car car, int num, String color)
	{
		car.num = num;
		car.color = color;
		System.out.println(car.num+"\t"+car.color);
	}
}
