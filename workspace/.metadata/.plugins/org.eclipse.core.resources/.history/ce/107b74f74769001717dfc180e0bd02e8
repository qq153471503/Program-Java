
class objectDemo{
	public static void main(String[] args){
		
		Persion per = new Persion();
	}
}

class Persion{

	private int num;
	public void setNum(int x){
		num = x;

	}
	
	Persion(){
		System.out.println("Persion run !");
	}
	Persion(int x){
		System.out.println("Persion run !"+"x is :"+x);
	}
}

