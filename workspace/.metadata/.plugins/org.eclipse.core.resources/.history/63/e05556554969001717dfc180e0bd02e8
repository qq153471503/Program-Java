
class objectDemo{
	public static void main(String[] args){
		
		Persion per = new Persion(10);
		Persion per2 = new Persion("LiuAnkun");
	}
}

class Persion{
	private int age;
	private String name;
	public void setAge(int x){
		age = x;
	}
	public int getAge(){
		return age;
	}
	
	
	public void speak(){
		System.out.println("num is :"+age+"\t"+"name is :"+name);
	}
	
	Persion(){
		System.out.println("Persion run !");
	}
	Persion(int x){
		System.out.println("Persion run !"+"\t"+"x is :"+x);
	}
	Persion(String str){
//		System.out.println("Persion run !"+"\t"+"String is :"+str);
		this.speak();
	}
}

