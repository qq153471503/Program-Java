public class ListDemo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		List l = new ArrayList();
		show(l);
	}

	private static void show(List l) {
		l.add("abc1");
		l.add("abc2");
		l.add("abc3");
		System.out.println(l);
		
		l.add(1, "123");
	}

}



