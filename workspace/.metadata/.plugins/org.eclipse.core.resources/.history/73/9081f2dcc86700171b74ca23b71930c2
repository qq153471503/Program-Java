public class hello{
	
	public static void main(String[] args){
		
		
		int y;
		int x = 1;
		
		y = (x>0)?100:200;
		
		System.out.println("y="+y);
		
		{
			int k = 10;
			System.out.println("x is : "+k);
		}
		
		
		{
			int t=1;
			int tmp=0;
			while(t <= 3)
			{
				tmp += t;
				t++;
				System.out.println("tmp is : "+tmp);
			}
		}
		
		{
			short t0 = 0;
			short t2=0;
			while(t0 <= 100)
			{
				t0++;
				if(t0 % 6 == 0)
				{
					System.out.println("t0 is : "+t0);
					t2++;
				}
			}
			System.out.println("t2 is : "+t2);
		}
		
		for(int x=0; x<10; x++)
		{
			System.out.println("x is : "+x);
		}
	}
}
