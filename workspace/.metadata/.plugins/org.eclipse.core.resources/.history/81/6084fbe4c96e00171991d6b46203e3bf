}

interface Graphics{
	abstract public double circularArea();
	abstract public double rectangularArea(); 
	abstract public double area();
}

class Circular implements Graphics{
	
	private double pai = 3.141592;
	private double r;
	
	Circular(double r){
		this.r = r;
	}
	public double area(){
		return this.r*this.r*this.pai;
	}
}

class Rectangular implements Graphics{
	private double x;
	private double y;
	Rectangular(double x, double y){
		this.x = x;
		this.y = y;
	}
	
	public  double area(){
		return this.x*this.y;
	}
}

